{
  "name": "ros-engine",
  "version": "1.0.4",
  "description": "NodeJs kub Proxy",
  "main": "./dist/server.js",
  "scripts": {
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "clean": "rm -rf node_modules/",
    "grunt": "grunt",
    "build": "rimraf ./dist && npm run lint && grunt",
    "start": "node ./dist/server.js",
    "watch": "cross-env NODE_ENV=dev PORT=8080 nodemon",
    "lint": "tslint ./src/**/*.ts",
    "mock:api": "json-server --watch ./resources/mock-request-kub.json --port 8000",
    "tslint": "tslint",
    "dev": "concurrently -k -n \"Typescript,NodeJS\" -p \"[{name}]\" -c \"blue,green\" \"tsc --watch\" \"nodemon dist/server.js\""
  },
  "keywords": [
    "ros-engine"
  ],
  "author": "Iloutfi",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^3.4.35",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/errorhandler": "0.0.32",
    "@types/method-override": "0.0.31",
    "@types/mocha": "^5.2.7",
    "@types/morgan": "^1.7.36",
    "@types/node": "^10.11.3",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "grunt": "^1.0.3",
    "grunt-available-tasks": "^0.6.3",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-ts": "^6.0.0-beta.19",
    "nodemon": "^1.18.4",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "tslint": "^5.13.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "chai": "^3.5.0",
    "chai-http": "^4.3.0",
    "chokidar": "^3.2.1",
    "config": "^3.2.0",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-generator": "^4.16.1",
    "express-status-monitor": "^1.2.3",
    "glob": "^7.1.4",
    "http-proxy": "^1.17.0",
    "http-proxy-middleware": "^0.19.1",
    "install": "^0.12.2",
    "json-server": "^0.15.0",
    "lodash": "^4.17.15",
    "method-override": "^3.0.0",
    "mocha": "^6.0.2",
    "mocha-typescript": "^1.0.23",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "node-schedule": "^1.3.2",
    "npm": "^6.10.2",
    "prom-client": "^11.5.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "request-promise-native": "^1.0.7",
    "response-time": "^2.3.2",
    "typescript": "3.0.3",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  }
}
